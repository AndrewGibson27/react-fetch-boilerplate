{
  "name": "react-webpack-express-isomorphic-boilerplate",
  "version": "1.0.0",
  "description": "Render on the server and client with Webpack, React, React Router and Express",
  "author": "Andrew Gibson <atgibson27@gmail.com>",
  "main": [
    "src/entry/index.js",
    "src/shared/routes.js",
    "src/server/index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AndrewGibson27/react-webpack-express-isomorphic-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/AndrewGibson27/react-webpack-express-isomorphic-boilerplate/issues"
  },
  "scripts": {
    "routes": "webpack --config webpack.config.server.js",
    "compile": "babel ./src/server/index.js --out-file ./server/server.js",
    "clean": "rimraf -f ./public/*build*",
    "dev": "npm run clean && npm run routes && npm run compile && npm run hot",
    "hot": "NODE_ENV=development node ./server/server.js",
    "webpack": "webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run routes && npm run compile && npm run webpack",
    "start": "NODE_ENV=production node ./server/server.js"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-cli": "^6.22.2",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-register": "^6.22.0",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "isomorphic-loader": "^1.6.1",
    "node-sass": "^4.4.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-loader": "^1.2.2",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "trash-cli": "^1.4.0",
    "uglify-save-license": "^0.4.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0"
  },
  "dependencies": {
    "babel-register": "^6.22.0",
    "express": "^4.14.0",
    "pug": "^2.0.0-beta8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  },
  "license": "MIT"
}
