{
  "name": "react-router-express-isomorphic-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate for building an isomorphic Node.js application powered by Webpack, React and React Router",
  "author": "Andrew Gibson <atgibson27@gmail.com>",
  "main": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "scripts": {
    "node-watch": "NODE_ENV=server-dev nodemon --exec npm run babel-node -- ./server/dev/main.js",
    "dev-watch": "npm run node-watch & NODE_ENV=client-dev webpack-dev-server --inline --hot",
    "node-no-watch": "NODE_ENV=server-dev npm run babel-node -- ./server/dev/main.js",
    "dev-no-watch": "npm run node-no-watch & NODE_ENV=client-dev webpack-dev-server --inline --hot",
    "babel-node": "babel-node",
    "build-client": "NODE_ENV=client-prod webpack --config webpack.config.prod.js",
    "build-node": "NODE_ENV=server-prod babel -d ./server/prod ./server/**",
    "build-server-react": "NODE_ENV=client-prod babel -d ./build/components ./src/components",
    "build": "npm run clean && npm run build-client && npm run build-node && npm run build-server-react",
    "start": "node server/prod/main.js",
    "clean": "rimraf -f build && rimraf -f server/prod && rimraf -f assets/img/prod && rimraf -f assets/bundle.min.js"
  },
  "devDependencies": {
    "babel-cli": "^6.6.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "i": "^0.3.4",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.1",
    "npm": "^3.8.0",
    "proxy-middleware": "^0.15.0",
    "react-transform-hmr": "^1.0.2",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1",
    "webpack-isomorphic-tools": "^2.2.31"
  },
  "license": "MIT",
  "dependencies": {
    "express": "^4.13.4",
    "jade": "^1.11.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  }
}
