{
  "name": "react-fetch-boilerplate",
  "version": "0.1.0",
  "description": "A boilerplate around React Router v4 and Heroku's react-refetch",
  "author": "Andrew Gibson <atgibson27@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/AndrewGibson27/react-fetch-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/AndrewGibson27/react-fetch-boilerplate/issues"
  },
  "scripts": {
    "start": "NODE_ENV=development node -r \"babel-register\" ./server",
    "start:prod": "NODE_ENV=production node ./build/server.js",
    "build": "webpack --config ./webpack/client.prod.js && webpack --config ./webpack/server.prod.js",
    "clean": "rm -rf build && rm -rf public/assets/"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "rimraf": "^2.6.1",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "mongoose": "^4.11.7",
    "pug": "^2.0.0-rc.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-refetch": "^1.0.0",
    "react-router-dom": "^4.1.2",
    "styled-components": "^2.1.2"
  },
  "license": "MIT"
}
